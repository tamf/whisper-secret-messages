{
  "functions": {
    "predeploy": [],
    "source": "functions"
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/",
        "destination": "/index.html"
      },
      {
        "source": "/create",
        "function": "create"
      },
      {
        "source": "/fetch",
        "function": "fetch"
      },
      {
        "source": "/delete",
        "function": "delete"
      }
    ]
  }
}